<h2>マイページ編集</h2>

<div class="main-container_user">
<%= form_with model: @user do |form| %>

  <div class="field_user_image">マイページ画像：<%= form.file_field :image , class:"btn btn-success" %></div>
  <br>
  <br>
  <div>ユーザー名：<%= form.text_field :username %></div>
  <br>
  <div>パスワード：<%= form.text_field :password %></div>
  <br>

  <div><%= form.submit "保存", class:"btn btn-warning"%></div>
  <br>
<% end %>
</div>
