<h2>マイページ編集</h2>

<div class="main-container_user">
<%= form_with model: @user do |form| %>

  <div class="field_user_image">マイページ画像：<%= form.file_field :image %></div>
  <div>ユーザー名：<%= form.text_field :username %></div>
  <div>パスワード：<%= form.text_field :password %></div>

  <div><%= form.submit "保存" %></div>
<% end %>
</div>
